/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./",
     "paths": {
        "@models/*": ["src/app/models/*"],
        "@core/*": ["src/app/core/*"],
        "@shared/*": ["src/app/shared/*"],
        "@components/*": ["src/app/components/*"],
        "@features/*": ["src/app/features/*"],
     }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
